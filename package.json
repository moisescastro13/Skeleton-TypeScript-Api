{
  "name": "node-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "start:dev": "nodemon --watch src/** --ignore 'src/**/*.spec.ts' --exec ts-node src/index.ts",
